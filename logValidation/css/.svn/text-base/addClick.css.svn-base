.tongji-table {
	width: 300px;
	border-collapse: collapse;
}

.tongji-table th,.tongji-table td {
	vertical-align: top;
	font-size: 13px;
	line-height: 150%;
}

.tongji-table-form td {
	padding: 2px 0;
	text-align: left;
	font-size: 14px;
}

.tongji-table-form td:first-child {
	padding-right: 10px;
	text-align: right;
	white-space: nowrap;
}

.tongji-table td.tongji-control {
	text-align: left;
}

.tongji-label {
	position: absolute;
	top: 10px;
	left: 1px;
	padding: 2px;
	border: 1px solid #c00;
	background: #e8cbcb;
	color: #000;
	white-space: nowrap;
	font-weight: normal;
	font-size: 12px;
	line-height: 12px;
	cursor: pointer;
	z-index: 100;
}

.tongji-label:hover {
	z-index: 1000;
}

.tongji-zindex {
	z-index: 100;
}

.tongji-ele-hilight {
	-webkit-mask: -webkit-gradient(linear, left top, left bottom, from(rgba(0,0,0,1)), to(rgba(0,0,0,0)));
	background: #3c8ce7;
}

