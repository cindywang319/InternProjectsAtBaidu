.container {
	position: absolute;
	background: #f8f8f8;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	border: 1px solid #ddd;
	padding: 3px;
	width: 300px;
	top: 32px;
	left: 75%;
}

.sub_container {
	position: absolute;
	background: #f8f8f8;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	border: 1px solid #ddd;
	padding: 3px;
	width: 310px;
	top: 32px;
	left: 3px;
}

.sub_button {
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	border: 1px solid #ccc;
	background: #f6f6f6;
	font-weight: bold;
	color: #1c94c4;
	left: 80%;
	padding: 5px 10px;
	font-size: 14px;
	position: relative;}

.titlebar {
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	padding:8px;
	border: 1px solid #B452CD;
	background: #CDB5CD;
	color: #fff;
	font-weight: bold;
	text-align: left;
	font-size: 15px;
}

.footer {
	text-align: left;
	background-image: none;
	margin-top: .5em;
	padding: .3em 1em .5em .4em;
	border-top: 1px solid #ddd;
	background: #eee;
	color: #333;
}

.button {
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	border-bottom-left-radius: 4px;
	border-bottom-right-radius: 4px;
	border: 1px solid #ccc;
	background: #f6f6f6;
	font-weight: bold;
	color: #1c94c4;
	left: 60%;
	padding: 5px 10px;
	font-size: 14px;
	position: relative;
}

a {
	color:#3D3D3D;
	text-decoration:underline;
}

.tongji-res-cont {
	position: relative;
	overflow-y: scroll;
	max-height: 430px;
	border: 1px solid #acceea;
}

.tongji-table-list th,.tongji-table-list td {
	padding: 0px 2px;
	border: 1px solid #acceea;
	border-bottom: none;
	border-left: none;
	text-align: center;
	white-space: nowrap;
}